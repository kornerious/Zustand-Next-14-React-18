{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "printWidth": 140,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "proseWrap": "preserve",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": "*.scss",
            "options": {
                "tabWidth": 2
            }
        }
    ],
    "importOrder": ["^react", "^next", "^[a-z]", "^@mui/(.*)$", "^@resident-brands/(.*)$", "^[./]"],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true
}
